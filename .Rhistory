model4 = lm(Score ~ Age, data = df)
summary(model4)
ggplot(df, mapping = aes()) +
geom_point(mapping = aes(x = Age, y = Score)) +
facet_wrap(~Gender)
kout = kmeans(df, 5, iter.max = 30, nstart = 20)
kout
n = 10
wss = numeric(n)
